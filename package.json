{
  "name": "youtube-subtitles-ts",
  "version": "1.0.1",
  "description": "Fetch YouTube video transcripts and subtitles. No API key required. Supports SRT, WebVTT, multiple languages, auto-generated captions, and translations.",
  "keywords": [
    "youtube",
    "transcript",
    "subtitles",
    "captions",
    "api",
    "typescript",
    "srt",
    "webvtt",
    "closed-captions",
    "video",
    "yt",
    "node",
    "nodejs",
    "scraper",
    "auto-generated",
    "translation",
    "innertube"
  ],
  "homepage": "https://github.com/Jonatna0990/youtube-subtitles-ts#readme",
  "bugs": {
    "url": "https://github.com/Jonatna0990/youtube-subtitles-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jonatna0990/youtube-subtitles-ts.git"
  },
  "license": "MIT",
  "author": "Gazaryan Georgie",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./formatters": {
      "types": "./dist/formatters.d.ts",
      "import": "./dist/formatters.mjs",
      "require": "./dist/formatters.js"
    },
    "./proxies": {
      "types": "./dist/proxies.d.ts",
      "import": "./dist/proxies.mjs",
      "require": "./dist/proxies.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fast-xml-parser": "^4.3.0",
    "he": "^1.2.0",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/he": "^1.2.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "module": "dist/index.mjs"
}
